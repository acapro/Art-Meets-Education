%section.project
  %aside
    %h1 Projekte
    %ul
      - project_by_year(@projects).each do |year, projects|
        %li= year.strftime("%Y")
        %ul.sub
          - projects.each do |project|
            = sub_link "#{project.city} - #{project.slum}", page_project_path(project)
  %article
    = render 'sections/kids', project: @project
