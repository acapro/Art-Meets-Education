%section.project
  %aside
    %h1 Projekte
    %ul
      - project_by_year(@projects).each do |year, projects|
        %li= year.strftime("%Y")
        %ul.sub
          - projects.each do |project|
            %li= "#{project.city} - #{project.slum}"
  %article
    = render 'sections/kids', project: @project
